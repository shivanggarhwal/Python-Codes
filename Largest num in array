#Largest element in an array
def large_arr(arr):
    n=len(arr)
    max1=arr[0]
    for i in range(0,n):
        for j in range(1,n):
#             print(max1)
            if max1<arr[j]:
                max1=arr[j]
                
    return max1
arr=[38,90,900786651,3556,9,1]      
large_arr(arr)
